{
	"static": {
		"style": {
			"position": "static"
		}
	},
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-red-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(254 202 202 / var(--tw-bg-opacity))"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	}
}